pipeline{
    agent{label 'shopizer'}
     triggers { cron( '2 7 * * *') }
    parameters { string(name:'mvncmd',defaultValue:'package',description:'build goal')}
    stages{
        stage('vcs'){
          
            steps{
                step{
                    git url:'https://github.com/saisatyateja/my-shopizer.git',
                    branch: 'rel'
                }
                step{
                    sh 'git merge --no-ff dev' 
                    sh 'git push -u origin rel'
                }
                
            }
        }
        }
}