pipeline {
    agent {label 'JDK11'} 
    triggers {
        cron('30 17 * * *')
    }
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/udaykiran-dhatrika/shopizer.git' , branch : 'release' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}